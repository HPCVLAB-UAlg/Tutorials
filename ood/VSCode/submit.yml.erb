---
batch_connect:
  template: "basic"          # one-port web proxy

script:
  queue_name:     "<%= custom_queue.strip %>"
  accounting_id:  "<%= auto_accounts.strip %>"   # OOD stores the value here
  time:           "<%= bc_num_hours %>:<%= '%02d' % bc_num_minutes %>:00"
  tasks:          1
  cpus_per_task:  "<%= num_cores %>"
  exclusive:      false